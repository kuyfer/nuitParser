Confluence.Templates.TextField.input = function(opt_data, opt_ignored) {
  var output = '';
  var baseType__soy148 = '';
  switch (opt_data.type) {
    case 'password':
    case 'email':
    case 'url':
    case 'tel':
    case 'search':
      baseType__soy148 += 'text';
      break;
    case 'submit':
    case 'reset':
      baseType__soy148 += 'button';
      break;
    default:
      baseType__soy148 += soy.$$escapeHtml(opt_data.type);
  }
